  display: flex;
  flex-direction: column;
  justify-content: space-between;
  height: 100%;
}

.wrapper {
  flex-grow: 1;
  height: 100%;
  overflow: auto;
}

.btn-group {  
  margin: 0 auto 2vh;
  text-align: center;
}

.btn-group button {
  background: #00000070;
  border-radius: 4px;
  border: 2px solid var(--secondary-color);
  box-shadow: 2px 2px rgba(0,0,0,0.2), 2px 2px rgba(0,0,0,0.19);
  color: var(--secondary-color);
  font-family: transat, sans-serif;
  font-size: 3vh;
  font-weight: 900;
}

h1#ui-dialog-title {
  background-color: var(--background-light-color);
}

#ui-dialog-body {
  background-color: var(--background-light-color);
}

#ui-dialog-close {
  background-color: var(--background-light-color);
  border-color: #000;
}

#ui-dialog-close:hover {
  background-color: var(--text-hover-color);
  border-color: var(--text-hover-color);
}

#passages {
  height: 100%;
}

#menu-item-share, #menu-item-saves, #menu-item-settings {
  display: none;
}

#menu li a {
  display: block;
  padding: .25em .75em;
  border: 1px solid transparent;
  color: var(--text-light-color);
  text-transform: uppercase;
  font-size: 3vh;
}

#ui-dialog-body {
  background-color: var(--background-color);
}

h1#ui-dialog-title {
  background-color: var(--background-color);
}

#ui-dialog-body {
  background-color: var(--background-color);
}

#ui-dialog-body.list li a {
  color: var(--text-light-color);
}

#ui-dialog-body.list li a:hover {
  background: transparent;
  border:none;
}

#story-title {
  display: none;
}

#ui-bar {
  background-color: transparent;
  border-right: 0;
  text-align: center;
}

#ui-bar-toggle:before {
  content: "\e81d";
  color: var(--secondary-color);
}

#refresh {
  content:"e5d5";
}

#back {
  content:"e5c4";
}

#yes {
  content:"e5ca";
}

#no {
  content:"e5cd";
}

#ui-bar-body {
  height: 100%;
  background-color: var(--background-color);
  background-image: url('images/menu.jpg');
  background-position: center;
  margin: 0;
  width: 34vh;
  border-right: solid 1px;
}

#ui-bar-history {
  display: none;
}
  
#ui-bar-tray {
    position: absolute;
}

#ui-bar-toggle {
  font-size: 4vh;
  line-height: inherit;
  color: var(--text-light-color);
  position: fixed;
  left:0;
  right: unset;
  top: unset;
}

#ui-bar-toggle:hover {
  background-color: transparent;
  border: none;
}

#ui-bar.stowed~#story {
  margin-left: 0;
}

#ui-bar.stowed #ui-bar-toggle:before {
  content: "\e81e";
  color: var(--secondary-color);
}

#ui-bar.stowed #ui-bar-toggle {
  font-size: 4vh;
}

button#history-forward {
  color: var(--text-light-color);
}

#history-forward hover {
  background-color: transparent;
  border: none;
}

button#history-backward {
  color: var(--text-light-color);
}

#ui-bar-history [id|=history]:hover {
  background-color: transparent;
}

#ui-bar-history [id|=history], #ui-bar-toggle {
  background-color: transparent;
  border: none;
}

#ui-bar-history [id|=history]:disabled {
  color: #E3E3E3;
  background-color: transparent;
  border-color: #000;
  border: 2px solid #000;
  border-radius: 4px;
}

#story-header {
	background-color: yellow;
	color: black;
	height: 10vh;
  text-align: center;
  text-align: vertical;
  word-wrap: break-word;
  padding: 10px;
  background: var(--background-light-color);
  border-bottom: 2px solid #362F2A;
}

#story {
	height: 100vh;
	background-position: center;
	background-repeat: no-repeat;
	background-size: cover;
	padding: 4vh 0;
	margin: 0;
}